import com.hcm.entity.Claim;

import lombok.Data;

@JsonInclude(JsonInclude.Include.NON_NULL)
@JsonPropertyOrder({
"claims"
})
@Data
public class SubmitClaimRequest {
	
//	@JsonProperty( "parameter" )
//	@XmlElement( required = true )
	@JsonProperty("claims")
	private List<Claim> claims;

}
